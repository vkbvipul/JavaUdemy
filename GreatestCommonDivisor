public class GreatestCommonDivisor {
    // write your code here
    public static int getGreatestCommonDivisor(int first,int second)
    {
        if((first < 10)||(second < 10))return -1;
        int i = 2;
        int gcd = 1;
        while(i<=first && i<=second)
        {
            if((first%i==0)&&(second%i==0))
            gcd = i;
            i++;
        }
    
        return gcd;
    }
}
